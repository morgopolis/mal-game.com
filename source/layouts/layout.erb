<!doctype html>
<html>

  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">
    <title><%= current_page.data.title %></title>
    <%= stylesheet_link_tag :site %>
    <%= javascript_include_tag :all %>
  </head>

  <body class="<%= page_classes %>">
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
      ga('create', '<%= config[:analytics_token] %>', 'auto');
      ga('send', 'pageview');
    </script>

    <footer>
      <div id="griffon" title="<%= config[:business_name] %>"></div>
      <a href="/" id="footer-title-image"><div class="title-image" title="<%= config[:business_name] %>"></div></a>
      <nav>
        <ul>
          <li><%= link_to '', '/press.html', :class => "press" %></li>
          <li><%= link_to '', '/music.html', :class => "music" %></li>
          <li><%= link_to '', '/credits.html', :class => "credits" %></li>
          <li><%= link_to '', '/contact.html', :class => "contact" %></li>
          <li><a href="<%= config[:url_reddit] %>" title="reddit" class="reddit" title="Visit our subreddit"></a></li>
          <li><a href="<%= config[:url_twitter] %>" title="twitter" class="twitter" title="Send us a tweet"></a></li>
          <li><a href="<%= config[:url_facebook] %>" title="facebook" class="facebook" title="Like us on Facebook"></a></li>
        </ul>
      </nav>
    </footer>

    <section id="outlet">
      <%= yield %>
      <div id="copyright">&copy; <%= config[:copyright_years] %> <%= config[:business_name_full] %>. All rights reserved.</div>
    </section>
  </body>
</html>



